{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "endOfLine": "auto",
  "importOrder": [
    "^[./].*\\.(css|scss|less)$",
    "^(fs|path|querystring|url|http|https|os|util|crypto|stream|buffer|events|child_process|cluster|dns|net|tls|dgram|readline|repl|string_decoder|tty|vm|zlib)$",
    "^next/",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^\\.\\./",
    "^\\./"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
